{
    "version": 2,
    "builds": [
    {
        "src": "emtrees_unofficial/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
        "distDir": "staticfiles_build"
        }
    },
    {
    "functions": {
        "api/**/*.py": {
        "excludeFiles": "public/**"
        }
    }
    }
    ],
    "routes": [
    {
        "src": "/static/(.*)",
        "dest": "/static/$1"
    },
    {
        "src": "/(.*)",
        "dest": "emtrees_unofficial/wsgi.py"
    }
    ]
}